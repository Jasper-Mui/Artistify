{
  "name": "artistify-frontend",
  "version": "0.11.0",
  "description": "search, play and queue music from youtube",
  "author": "artistify-team",
  "main": "webpack.config.js",
  "scripts": {
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm run react-build && electron-builder --publish=always",
    "build2": "npm run react-build && npm run electron-build",
    "start2": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron": "electron .",
    "electronpackager": "electron-packager .",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build": "webpack -p --config ./webpack.config.js --mode production",
    "lint": "eslint ./src/**/*.js ./src/**/*.jsx --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "axios": "^0.20.0",
    "babel-loader": "^8.1.0",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.2.0",
    "electron-packager": "^15.1.0",
    "immutability-helper": "^3.1.1",
    "query-string": "^6.13.5",
    "react": "^16.13.1",
    "react-device-detect": "^1.14.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dnd-touch-backend": "^11.1.3",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "react-select": "^3.1.0",
    "react-uuid": "^1.0.2",
    "react-youtube": "^7.12.0",
    "regenerator-runtime": "^0.13.7",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "electron-builder": "^22.8.1",
    "electron": "^10.1.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "husky": "^4.3.0",
    "prettier": "^2.1.2"
  }
}
